.page__bd {
  padding: 30rpx;
}
.bw_mapController__hd {
  display: flex;
  align-items: center;
}
.bw_mapController__title {
  padding-left: 54rpx;
  position: relative;
}
.bw_mapController__btn {
  background: white;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-radius: 20rpx;
  font-size: 10pt;
  padding: 5rpx 15rpx;
}
/* 伪元素 css声明，没有标签 可以页面显示 */
.bw_mapController__title::before {
  content: ''; /* 伪元素必要属性 */
  position: absolute;
  top: 0;
  left: 0;
  width: 54rpx;
  height: 54rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
}
.bw_mapController__bd {
  height: 300rpx;
  position: relative;
  transition: height 0.5s;
  overflow: hidden;
  margin-top: 20rpx;
}
.bw_mapController__bd map {
  width: 100%;
  height: 100%;
}
.bw_mapController__bd .bw_mapController__switch {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 54rpx;
  background-color: #fff;
  text-align: center;
}
.bw_mapController_bd-hide {
  height: 0;
}

